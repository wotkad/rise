{
  "name": "rise",
  "version": "1.0.0",
  "description": "rise",
  "author": {
    "name": "wotkad",
    "email": "wotkad@gmail.com",
    "url": "https://wotkad.ru"
  },
  "main": "src/bundle.js",
  "repository": "git@github.com:wotkad/rise.git",
  "license": "MIT",
  "lint-staged": {
    "src/**/*.scss": [
      "yarn --silent prettier --write --log-level silent './src/assets/styles/**/*.scss'",
      "yarn prettify:scss-files",
      "yarn prettify:stylelint"
    ],
    "src/**/*.pug": [
      "yarn prettify:pug-files",
      "yarn --silent prettier --write --log-level silent './src/views/**/*.pug'",
      "yarn prettify:pug-files",
      "yarn prettify:pug-mixins",
      "yarn prettify:js-defaults"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettify:staged"
    }
  },
  "scripts": {
    "build": "yarn --silent production",
    "production": "NODE_NO_WARNINGS=1 yarn --silent production:sequence",
    "production:sequence": "rimraf build && webpack --env mode=production --config ./webpack.config --progress && yarn --silent production:metatag && yarn --silent reports:performance && yarn --silent reports:lighthouse && yarn --silent reports:a11y && yarn --silent production:service-worker",
    "production:metatag": "node ./plugins/production/metatag",
    "production:service-worker": "node ./plugins/production/service-worker",
    "production:markdown": "yarn --silent rimraf ./src/views/pages/blog/ && node ./plugins/production/markdown.mjs",
    "dev": "yarn --silent development",
    "development": "NODE_NO_WARNINGS=1 concurrently --raw \"yarn --silent development:sequence\" \"node ./plugins/optimization/images.js\"",
    "development:sequence": "yarn --silent reports:content && yarn --silent production:markdown && webpack serve --env mode=development --config ./webpack.config --progress",
    "prettify:scss": "yarn --silent prettier --write --log-level silent './src/assets/styles/**/*.scss' && yarn --silent prettify:scss-files && yarn --silent prettify:stylelint",
    "prettify:pug": "yarn --silent prettify:pug-files && yarn --silent prettier --write --log-level silent './src/views/**/*.pug' && yarn --silent prettify:pug-files && yarn --silent prettify:pug-mixins && yarn --silent prettify:js-defaults",
    "prettify:all": "yarn --silent prettify:svg && yarn --silent prettify:scss && yarn --silent prettify:pug",
    "prettify:stylelint": "stylelint 'src/assets/styles/**/*.scss'",
    "prettify:svg": "node ./plugins/prettify/svg",
    "prettify:scss-files": "node ./plugins/prettify/scss-files",
    "prettify:pug-files": "node ./plugins/prettify/pug-files",
    "prettify:pug-mixins": "node ./plugins/prettify/pug-mixins",
    "prettify:js-defaults": "node ./plugins/prettify/js-defaults",
    "prettify:staged": "lint-staged",
    "prettify:linkinator": "node ./plugins/prettify/linkinator.mjs",
    "reports:performance": "node ./plugins/reports/performance",
    "reports:lighthouse": "node ./plugins/reports/lighthouse",
    "reports:a11y": "node ./plugins/reports/a11y",
    "reports:content": "node ./plugins/reports/content",
    "constructor": "node ./plugins/constructor/constructor",
    "constructor:preview": "yarn --silent preview",
    "preview": "node ./plugins/constructor/preview"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.21.0",
    "@barba/core": "^2.10.0",
    "@prettier/plugin-pug": "^3.4.2",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@webdiscus/pug-loader": "^2.10.5",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^3.0.4",
    "browser-sync-webpack-plugin": "^2.4.0",
    "chalk": "^5.2.0",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.29.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "enhanced-resolve": "^5.12.0",
    "fast-glob": "^3.3.3",
    "favicons": "^7.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.0",
    "glob": "^11.0.3",
    "glob-all": "^3.3.1",
    "highlight.js": "^11.11.0",
    "html-webpack-plugin": "5.0.0",
    "html2pug": "^4.0.0",
    "http-server": "^14.1.0",
    "husky": "^9.1.7",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webpack-plugin": "^2.4.2",
    "lighthouse": "^13.0.1",
    "linkinator": "^5.0.1",
    "lint-staged": "^16.2.6",
    "markdown-it": "^14.1.0",
    "markdown-it-meta": "^0.0.1",
    "mini-css-extract-plugin": "^2.7.2",
    "pa11y": "^9.0.1",
    "postcss": "^8.4.12",
    "postcss-cli": "^10.1.0",
    "postcss-import": "15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-modules": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "pug": "3.0.2",
    "pug-loader": "^2.4.0",
    "puppeteer": "^24.26.1",
    "purgecss-webpack-plugin": "^7.0.2",
    "rimraf": "^4.1.3",
    "robotstxt-webpack-plugin": "^8.0.0",
    "sass": "^1.93.2",
    "sass-loader": "^13.2.0",
    "sitemap": "^8.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.1.0",
    "tailwindcss": "^3.4.17",
    "terser-webpack-plugin": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.102.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "inputmask": "^5.0.9",
    "jquery": "^3.7.1",
    "scroll-lock": "^2.1.5",
    "scrollreveal": "^4.0.9",
    "smooth-scroll": "^16.1.3",
    "swiper": "^12.0.3"
  }
}
